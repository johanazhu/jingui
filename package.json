{
  "name": "@jojobo/jing-ui",
  "version": "0.0.8",
  "tags": [
    "jing",
    "jing-ui",
    "react",
    "react-jing",
    "jingqb"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "jing",
    "jing-ui",
    "jing-mobile",
    "component",
    "components",
    "ui",
    "frontend",
    "mobile"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father build",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "release": "pnpm run build && pnpm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,scss,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.min.js",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.sass",
    "./es/index.js",
    "./es/Styles/index.js",
    "lib/**/Styles/*",
    "src/**/Styles/**"
  ],
  "lint-staged": {
    "*.{js,jsx,scss,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "homepage": "https://jingui.azhubaby.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:johanazhu/jingui.git"
  },
  "license": "MIT",
  "author": "jojobo",
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "dependencies": {
    "@better-scroll/core": "^2.5.0",
    "classnames": "^2.3.2",
    "color": "^4.2.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "swiper": "^8.4.3"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/color": "^3.0.3",
    "@types/lodash": "^4.14.186",
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "^4.0.24",
    "dumi": "^1.1.48",
    "dumi-theme-mobile": "^1.1.25",
    "father": "^4.1.3",
    "gh-pages": "^4.0.0",
    "lint-staged": "^13.0.3",
    "lorem-ipsum": "^2.0.8",
    "postcss-url": "^10.1.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  }
}
