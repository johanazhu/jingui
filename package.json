{
  "private": true,
  "name": "jing-ui",
  "version": "0.0.1",
  "tags": [
    "jing",
    "jing-ui",
    "react",
    "react-jing",
    "jingqb"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "jing",
    "jing-ui",
    "jing-mobile",
    "component",
    "components",
    "ui",
    "frontend",
    "mobile"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,scss,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,scss,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@antv/f2": "^3.8.9",
    "classnames": "^2.3.1",
    "react": "^16.14.0",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/react-dom": "^17.0.9",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.16",
    "dumi-theme-mobile": "^1.1.6",
    "father-build": "^1.17.2",
    "gh-pages": "^3.2.3",
    "lint-staged": "^10.0.7",
    "node-sass": "^6.0.1",
    "prettier": "^2.2.1",
    "sass-loader": "^12.1.0",
    "yorkie": "^2.0.0"
  }
}
