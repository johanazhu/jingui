{
  "private": true,
  "name": "jing-ui",
  "version": "0.0.1",
  "tags": [
    "jing",
    "jing-ui",
    "react",
    "react-jing",
    "jingqb"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "jing",
    "jing-ui",
    "jing-mobile",
    "component",
    "components",
    "ui",
    "frontend",
    "mobile"
  ],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,scss,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,scss,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@better-scroll/core": "^2.4.2",
    "classnames": "^2.3.1",
    "color": "^4.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swiper": "^7.3.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^3.1.0",
    "@types/color": "^3.0.2",
    "@types/react-dom": "^17.0.10",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "^3.5.20",
    "dumi": "^1.1.31",
    "dumi-theme-mobile": "^1.1.14",
    "father-build": "^1.20.1",
    "gh-pages": "^3.2.3",
    "lint-staged": "^11.2.6",
    "lorem-ipsum": "^2.0.4",
    "prettier": "^2.4.1",
    "yorkie": "^2.0.0"
  }
}
